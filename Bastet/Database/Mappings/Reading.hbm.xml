<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Bastet" namespace="Bastet.Database.Model">
  <class name="Reading" table="readings">
    <id name="Id">
      <generator class="guid" />
    </id>

    <property name="Timestamp" type="UtcDateTime"/>
    
    <many-to-one name="Sensor" />
  </class>
</hibernate-mapping>